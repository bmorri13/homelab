# Terraform Secrets Configuration
# Copy this file to 'secrets.tfvars' and update with your actual credentials

# Proxmox API credentials (SENSITIVE)
proxmox_api_token_id     = "root@pam!terraform"
proxmox_api_token_secret = "your-proxmox-api-token-secret"

# VM password (SENSITIVE)
vm_password = "your-secure-vm-password"