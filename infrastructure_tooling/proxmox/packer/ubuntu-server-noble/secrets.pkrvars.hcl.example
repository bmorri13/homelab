# Packer Secrets Configuration
# Copy this file to 'secrets.pkrvars.hcl' and update with your actual credentials

# Proxmox API token for Packer operations
proxmox_token = "your-proxmox-api-token-here"

# SSH password for the VM template
ssh_password = "your-secure-password"

# Tailscale auth key for auto-connect (reusable key recommended)
tailscale_auth_key = "tskey-auth-your-key-here"